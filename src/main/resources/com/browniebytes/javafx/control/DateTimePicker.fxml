<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">

	<TextField id="DateTimePicker_TextField" fx:id="textField" disable="true" focusTraversable="false" maxWidth="1.7976931348623157E308" />

	<Button id="DateTimePicker_Button" fx:id="button" onAction="#handleButtonAction" prefHeight="25.0">
		<graphic>
			<ImageView>
				<image>
					<Image url="@icons/calendar.png" />
				</image>
			</ImageView>
		</graphic>
	</Button>

	<stylesheets>
		<URL value="@DateTimePicker.css" />
	</stylesheets>

</fx:root>
